//# sourceMappingURL=bootstrap.min.js.map

/*!
 * jQuery Cookie Plugin v1.4.1
 * https://github.com/carhartl/jquery-cookie
 *
 * Copyright 2013 Klaus Hartl
 * Released under the MIT license
 */
(function (factory) {
	if (typeof define === 'function' && define.amd) {
		// AMD
		define(['jquery'], factory);
	} else if (typeof exports === 'object') {
		// CommonJS
		factory(require('jquery'));
	} else {
		// Browser globals
		factory(jQuery);
	}
}(function ($) {

	var pluses = /\+/g;

	function encode(s) {
		return config.raw ? s : encodeURIComponent(s);
	}

	function decode(s) {
		return config.raw ? s : decodeURIComponent(s);
	}

	function stringifyCookieValue(value) {
		return encode(config.json ? JSON.stringify(value) : String(value));
	}

	function parseCookieValue(s) {
		if (s.indexOf('"') === 0) {
			// This is a quoted cookie as according to RFC2068, unescape...
			s = s.slice(1, -1).replace(/\\"/g, '"').replace(/\\\\/g, '\\');
		}

		try {
			// Replace server-side written pluses with spaces.
			// If we can't decode the cookie, ignore it, it's unusable.
			// If we can't parse the cookie, ignore it, it's unusable.
			s = decodeURIComponent(s.replace(pluses, ' '));
			return config.json ? JSON.parse(s) : s;
		} catch(e) {}
	}

	function read(s, converter) {
		var value = config.raw ? s : parseCookieValue(s);
		return $.isFunction(converter) ? converter(value) : value;
	}

	var config = $.cookie = function (key, value, options) {

		// Write

		if (value !== undefined && !$.isFunction(value)) {
			options = $.extend({}, config.defaults, options);

			if (typeof options.expires === 'number') {
				var days = options.expires, t = options.expires = new Date();
				t.setTime(+t + days * 864e+5);
			}

			return (document.cookie = [
				encode(key), '=', stringifyCookieValue(value),
				options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
				options.path    ? '; path=' + options.path : '',
				options.domain  ? '; domain=' + options.domain : '',
				options.secure  ? '; secure' : ''
			].join(''));
		}

		// Read

		var result = key ? undefined : {};

		// To prevent the for loop in the first place assign an empty array
		// in case there are no cookies at all. Also prevents odd result when
		// calling $.cookie().
		var cookies = document.cookie ? document.cookie.split('; ') : [];

		for (var i = 0, l = cookies.length; i < l; i++) {
			var parts = cookies[i].split('=');
			var name = decode(parts.shift());
			var cookie = parts.join('=');

			if (key && key === name) {
				// If second argument (value) is a function it's a converter...
				result = read(cookie, value);
				break;
			}

			// Prevent storing a cookie that we couldn't decode.
			if (!key && (cookie = read(cookie)) !== undefined) {
				result[name] = cookie;
			}
		}

		return result;
	};

	config.defaults = {};

	$.removeCookie = function (key, options) {
		if ($.cookie(key) === undefined) {
			return false;
		}

		// Must not alter options, thus extending a fresh object...
		$.cookie(key, '', $.extend({}, options, { expires: -1 }));
		return !$.cookie(key);
	};

}));

'use strict';

var authTokenHandler = (function ($) {

    function getToken() {
        return $.cookie("apiToken");
    }

    function hasValidCredentials() {
        var token = getToken();
        if (typeof (token) == 'undefined' || token == null) return false;
        return true;
    }

    function clearAuthData() {
        $.removeCookie("apiToken", { path: '/' });
    }

    function addAuthHeader(xhr) {
        
        if (!hasValidCredentials()) {
            return;
        }

        var auth = getToken();
        var header = "Basic " + auth;
        xhr.setRequestHeader('Authorization', header);
    }

    function authErrorHandler(xhr) {
        console.log(xhr);
        if (xhr.status == '401' && hasValidCredentials()) {
            //clearAuthData();
        }
    }

    function init() {
        $.ajaxSetup({
            beforeSend: addAuthHeader,
            error: authErrorHandler
        });
    }

    init();

    return {
        getToken: getToken,
        hasValidCredentials: hasValidCredentials
    }

})(jQuery);
/*
Handles subscription and publishing of topics to avoid passing objects 
around and support not having to add named modules.

Last modified 2014-09-24 by ps.
*/

'use strict;'

var pubsub = (function () {

    var _subscriptions = [];
    var _topics = {};
    var _key = 0;

    var _publishing = 0;
    var _pendingKeys = [];

    function subscribe(topic, fn) {
        var keys = _topics[topic];
        if (!keys) {
            keys = _topics[topic] = [];
        }
        var key = _key;
        _subscriptions[key] = topic;
        _subscriptions[key + 1] = fn;
        _key = key + 2;
        keys.push(key);

        return key;
    }

    function unsubscribe(topic, fn) {
        var keys = _topics[topic];
        if (keys) {
            var subscriptions = _subscriptions;
            var foundKey = null;
            for (var i = 0, len = keys.length; i < len; i++) {
                if (subscriptions[i + 1] == fn) {
                    foundKey = i;
                }
            }
            if (foundKey >= 0) {
                return unsubscribeKey(foundKey);
            }
        }
        return false;
    }

    function unsubscribeKey(key) {

        // don't unsubscribe while publishing is occurring.
        // instead, add the unsub to a list of pending unsubs
        if (_publishing !== 0) {
            if (!_pendingKeys) {
                _pendingKeys = [];
            }
            _pendingKeys.push(key);
            return false;
        }
        // unsubscribe
        var topic = _subscriptions[key];
        if (topic) {
            var keys = _topics[topic];
            if (keys) {
                var i = keys.indexOf(key);
                if (i >= 0) {
                    keys.splice(i, 1);
                }
            }
            delete _subscriptions[key];
            delete _subscriptions[key + 1];
        }

        return !!topic;
    }

    function publish(topic, data) {
        var keys = _topics[topic];
        if (keys) {
            // lock to prevent unsubs
            _publishing++;

            // publish
            for (var i = 0, len = keys.length; i < len; i++) {
                var key = keys[i];
                _subscriptions[key + 1](topic, data);
            }

            // unlock
            _publishing--;
        }

        // process pending unsubscribes
        if (_pendingKeys && _publishing == 0) {
            var pendingKey;
            while ((pendingKey = _pendingKeys.pop())) {
                unsubscribeByKey(pendingKey);
            }
        }
    }

    function getSubscriptionCount(topic) {
        if (topic) {
            var keys = _topics[topic];
            return keys ? keys.length : 0;
        }

        var count = 0;
        for (var topic in _topics) {
            count += getSubscriptionCount(topic);
        }
        return count;
    }

    function clear() {
        _subscriptions.length = 0;
        _topics = {};
    }

    return {
        clear: clear,
        publish: publish,
        subscribe: subscribe,
        unsubscribe: unsubscribe,
        getSubscriptionCount: getSubscriptionCount
    }
})();
'use strict';

var urlHandler = (function (win) {

    var rootUrlElement = win.document.querySelector("link[rel='index']");
    var rootUrl = '';

    function init() {
        if (rootUrl.length == 0)
        {
            rootUrl = rootUrlElement.getAttribute("href");
        }
    }

    function getRoot() {
        return rootUrl;
    }

    function buildUrl(url) {
        if (url.charAt(0) == '/')
        {
            url = url.substr(1);
        }
        var url = rootUrl + url;
        return url;
    }

    init();

    return {
        getRoot: getRoot,
        buildUrl: buildUrl
    }

})(this);

'use strict'
; (function (win, $, urlHandler, pubsub) {

    var requestIntervalId = null;
    var internalElapsed = 0;

    var internalVarianceBeforeRealRequest = 30 // seconds
    var makeRequestInterval = 10000; // milliseconds
    var secondsRemainingBeforeWarning = 120; //seconds (recommend 120)

    // Sets up the interval-ed calls to track the seconds elapsed since the 
    // last request to getSessionTimeoutValue() (shortly after page load).
    function startRequestCalls(sessionTimeoutInSeconds) {
        if (requestIntervalId != null) {
            window.clearInterval(requestIntervalId);
        }
        requestIntervalId = window.setInterval(function () {
            checkIfRealRequestIsNecessary(sessionTimeoutInSeconds);
        }, makeRequestInterval);
    }
    // Handles the checking of the values (sessionTimeoutInSeconds and elapsedSeconds)
    // that results from the previous calls (respectively, getSessionTimeoutValue and 
    // getElapsedSinceRequest()). Depending on the values of those variables, the process 
    // will be restarted, the user will be logged out, the user will be warned, or nothing
    // will happen (any warning that happens to exist will be cancelled).
    function checkSessionTimeRemaining(sessionTimeoutInSeconds, elapsedSeconds) {

        if (elapsedSeconds <= 0) {
            // restart, user was logged out in a separate window/tab
            start();
            return;
        }

        var secondsUntilTimeout = sessionTimeoutInSeconds - elapsedSeconds;
        if (secondsUntilTimeout <= 0) {
            pubsub.publish("sessionExtender/end", {});
            endUserSession();
            if (requestIntervalId != null) {
                window.clearInterval(requestIntervalId);
            }
        }
        else if (secondsUntilTimeout < secondsRemainingBeforeWarning) {
            pubsub.publish(
                "sessionExtender/warn", 
                { 
                    "secondsRemaining" : secondsUntilTimeout,
                    "extendCallback": start,
                    "expireCallback" : endUserSession
                });
        }
        else 
        {
            pubsub.publish("sessionExtender/cancelWarn", {});
        }
    }
    // Determines (using an internal timer of elapsed seconds) if we need to bother 
    // communicating with the API to get the real elapsed value. This enables accuracy, 
    // but without constantly asking the server how many seconds have ACTUALLY elapsed.
    // No messages are presented to the user until the real value has been retrieved.
    function checkIfRealRequestIsNecessary(sessionTimeoutInSeconds)
    {
        if (internalElapsed >= (sessionTimeoutInSeconds - (internalVarianceBeforeRealRequest + secondsRemainingBeforeWarning)))
        {
            getElapsedSinceRequest(checkSessionTimeRemaining, sessionTimeoutInSeconds);
            return;
        }
        internalElapsed += (makeRequestInterval / 1000);
    }
    // Calls the API to determine what the session timeout is set to (in seconds)
    function getSessionTimeoutValue(callback) {
        var url = urlHandler.buildUrl("/api/webMethods/sessions.aspx/GetSessionTimeout");
        $.ajax({
            url: url,
            type: 'POST',
            contentType: "application/json; charset=utf-8",
            datatype: 'json',
            success: function (data) {
                if (data && data.d && typeof (data.d.timeout) !== 'undefined') {
                    var sessionTimeoutInSeconds = data.d.timeout;
                    if (window.console && window.console.log)
                    {
                        var sess_expire_time = new Date(new Date().getTime() + sessionTimeoutInSeconds * 1000).toLocaleTimeString();
                        window.console.log("Session will time out in " + sessionTimeoutInSeconds + " seconds (" + sess_expire_time + ")");
                    }
                    callback(sessionTimeoutInSeconds);
                }
            }
        });
    }
    // Calls the API to determine how many seconds have elapsed since the call to 
    // get the session timeout. Updates the internal tracker as well.
    function getElapsedSinceRequest(callback, sessionTimeoutInSeconds) {
        var url = urlHandler.buildUrl("/api/webMethods/sessions.aspx/GetSecondsElapsedSinceLastRequest");
        $.ajax({
            url: url,
            type: 'POST',
            contentType: "application/json; charset=utf-8",
            datatype: 'json',
            success: function (data) {
                if (data && data.d && typeof (data.d.elapsed) !== 'undefined') {
                    var elapsedSeconds = data.d.elapsed;
                    internalElapsed = elapsedSeconds;
                    callback(sessionTimeoutInSeconds, elapsedSeconds);
                }
            }
        });
    }
    // Calls the API to end the current session.
    function endUserSession() {
        var url = urlHandler.buildUrl("/api/webMethods/sessions.aspx/EndUserSession");
        $.ajax({
            url: url,
            type: 'POST',
            contentType: "application/json; charset=utf-8",
            datatype: 'json',
            success: function (data) {
                if (data && data.d && typeof (data.d.status) !== 'undefined') {
                    var status = data.d.status;
                }
            }
        });
    }

    function subscribe() {
        // provide a hook for pages to extend the session at will.
        pubsub.subscribe("sessionExtender/extend", start);
    }

    // Initialize the whole #!
    function init() {
        subscribe();
        start();
    }

    function start() {
        internalElapsed = (makeRequestInterval / 1000);
        getSessionTimeoutValue(startRequestCalls);
    }

    init();

})(this, jQuery, urlHandler, pubsub);
'use strict';

; (function (win, $, pubsub) {

    var countdownIntervalId = null;
    var CONST_CONTAINER_ID = "#sessionExtenderContainer";

    function getContainerElement() {
        return win.document.querySelector(CONST_CONTAINER_ID);
    }
    function getMessageElement() {
        return win.document.querySelector("#sessionExtenderMessage");
    }

    function renderDialog(title, text, button1Text, callBack1, button2Text, callBack2)
    {
        var el = getContainerElement();
        if (el === null) return;
     
        var $button1 = null;
        var $button2 = null;

        // construct the 'buttons'
        if (button1Text !== null && typeof (button1Text) !== 'undefined') {
            $button1 = $("<a class='btn btn-default button1' href='#'>").text(button1Text);
            if (callBack1 === null || typeof (callBack1) === 'undefined') {
                var button1EventHandler = function (e) { destroyDialog(); }
            } else {
                var button1EventHandler = function (e) {
                    callBack1();
                    destroyDialog();
                }
            }
            $button1.on('click', button1EventHandler);
        }

        if (button2Text !== null && typeof(button2Text) !== 'undefined') {
            $button2 = $("<a class='pseudo-button button2' href='#'>").text(button2Text);
            if (callBack2 === null || typeof (callBack2) === 'undefined') {
                var button2EventHandler = function (e) { destroyDialog(); }
            } else { 
                var button2EventHandler = function (e) { 
                    callBack2();
                    destroyDialog(); 
                }
            }
            $button2.on('click', button2EventHandler);
        }

        // construct the markup
        var $confirmationMessage = $(
			"<div class='popConfirmationWrapper'>" +
                "<div class='popConfirmationDialogWrapper'>" + 
    				"<div class='popConfirmationDialog'>" +
	    				"<div class='popConfirmationTitle'>" + title + "</div>" +
		    			"<div class='popConfirmationText'>" + text + "</div>" +
			    	"</div>" +
                "</div>" +
			"</div>");
        
        var $buttonWrapper = $("<div class='popConfirmationButtonWrapper'></div>");
        if ($button1 !== null) $buttonWrapper.append($button1);
        if ($button2 !== null) $buttonWrapper.append($button2);

        el.appendChild($confirmationMessage.get(0));
        $(".popConfirmationDialog").append($buttonWrapper);
    }

    function createDialog(msg, callback) {
        var title = "Session Timeout";
        var text = "<span id='sessionExtenderMessage'>" + msg + "</span>";
        var buttonText = "Continue";
        renderDialog(title, text, buttonText, callback);
    }
    
    function destroyDialog() {

        if (countdownIntervalId != null)
        {
            window.clearInterval(countdownIntervalId);
        }

        var el = getContainerElement();
        if (el === null) return;

        while (el.firstChild) {
            el.removeChild(el.firstChild);
        }
    }
    function updateDialog(msg) {
        var m = getMessageElement();
        if (m === null) return;
        m.textContent = msg;
    }

    function handleSessionEnd(topic, data) {
        destroyDialog();
        createDialog("Your session has ended.", redirectAfterTimeout);
    }
    function handleSessionWarn(topic, data) {

        var extendCallback = data.extendCallback;
        var expireCallback = data.expireCallback;
        var seconds = data.secondsRemaining;

        var m = getMessageElement();
        if (m === null) {
            var message = "Your Session will time out in " + seconds + " seconds. Once that occurs, you will be automatically logged out. If you'd like to continue, please click the 'Continue' button";
            createDialog(message, extendCallback);
        }

        if (countdownIntervalId === null && seconds > 0) {
            countdownIntervalId = window.setInterval(function () {

                seconds = seconds - 1;

                if (seconds <= 0) {
                    window.clearInterval(countdownIntervalId);
                }

                var message = "Your Session will time out in " + seconds + " seconds. Once that occurs, you will be automatically logged out. If you'd like to continue, please click the 'Continue' button";
                updateDialog(message);
            }, 1000);
        }
    }

    function redirectAfterTimeout() {
        window.location.href = urlHandler.getRoot();
    }

    function subscribe() {
        pubsub.subscribe("sessionExtender/end", handleSessionEnd);
        pubsub.subscribe("sessionExtender/warn", handleSessionWarn);
        pubsub.subscribe("sessionExtender/cancelWarn", destroyDialog);
    }

    subscribe();

})(this, jQuery, pubsub);
function adjustValidationSummary() {
    //$(".validation-summary-errors").removeClass("validation-summary-errors");
    $(".input-validation-error").removeClass("input-validation-error").parent().addClass("has-error");
}
/// <reference path="loadingDialog.js" />

function ajaxComplete(response, status, message) {
    if (response.responseText.lastIndexOf('document.location', 0) != 0)
    {
        hideLoadingDialog();
        adjustValidationSummary();        
    }
    else
    {
        showLoadingDialog(message);
    }
}
function showLoadingDialog() {
    showLoadingDialog(null);
}

function showLoadingDialog(loadingText)
{
    if (loadingText != null)
    {
        $('.loaderText').html(loadingText);
        $('.loaderText').show();
    }
    else
    {
        $('.loaderText').html('');
        $('.loaderText').hide();
    }

    $('#ajaxLoader').show();
}

function hideLoadingDialog()
{
    $('#ajaxLoader').hide();
}


/**
 * stacktable.js
 * Author & copyright (c) 2012: John Polacek
 * CardTable by: Justin McNally (2015)
 * Dual MIT & GPL license
 *
 * Page: http://johnpolacek.github.com/stacktable.js
 * Repo: https://github.com/johnpolacek/stacktable.js/
 *
 * jQuery plugin for stacking tables on small screens
 *
 */
;(function($) {
  $.fn.cardtable = function(options) {
    var $tables = this,
        defaults = {id:'stacktable small-only',hideOriginal:true,headIndex:0},
        settings = $.extend({}, defaults, options);

    // checking the "headIndex" option presence... or defaults it to 0
    if(options && options.headIndex)
      headIndex = options.headIndex;
    else
        headIndex = 0;

    var headerClassesToHide = [];
    if (options && options.headerClassesToHide)
        headerClassesToHide = options.headerClassesToHide;

    return $tables.each(function() {
      $table = $(this);
      if ($table.hasClass('stacktable')) {
        return;
      }
      var table_css = $(this).prop('class');
      var $stacktable = $('<div></div>');
      if (typeof settings.myClass !== 'undefined') $stacktable.addClass(settings.myClass);
      var markup = '';

      
      $table.addClass('stacktable large-only');
      $caption = $table.find("caption").clone();
      $topRow = $table.find('tr').eq(0);

      // using rowIndex and cellIndex in order to reduce ambiguity
      $table.find('tbody tr').each(function(rowIndex,value) {

        // declaring headMarkup and bodyMarkup, to be used for separately head and body of single records
        headMarkup = '';
        bodyMarkup = '';
        tr_class = $(this).prop('class');
        // for the first row, "headIndex" cell is the head of the table
        // for the other rows, put the "headIndex" cell as the head for that row
        // then iterate through the key/values
        $(this).find('td,th').each(function(cellIndex,value) {
          if ($(this).html() !== ''){
              bodyMarkup += '<tr class="' + tr_class + '">';
              var headerclass = $topRow.find('td,th').eq(cellIndex).attr('class');

              var hasHeaderClassToHide = false;
              if (headerclass && headerClassesToHide && (headerClassesToHide.indexOf(headerclass) > -1))
                  hasHeaderClassToHide = true;
            if ($topRow.find('td,th').eq(cellIndex).html() && hasHeaderClassToHide === false){
                bodyMarkup += '<td class="st-key">' + $table.find('thead th').eq(cellIndex).html() + '</td>';
                bodyMarkup += '<td class="st-val ' + $(this).prop('class') + '">' + $(this).html() + '</td>';
            } else {
                bodyMarkup += '<td colspan="2" class="st-val ' + $(this).prop('class') + '">' + $(this).html() + '</td>';
            }
            bodyMarkup += '</tr>';
          }
        });

        markup += '<table class=" '+ table_css +' '+settings.id+'"><tbody>' + headMarkup + bodyMarkup + '</tbody></table>';
      });

      $table.find('tfoot tr td').each(function(rowIndex,value) {
        if ($.trim($(value).text()) !== '') {
          markup += '<table class="'+ table_css + ' ' +settings.id+'"><tbody><tr><td>' + $(value).html() + '</td></tr></tbody></table>';
        }
      });

      $stacktable.prepend($caption);
      $stacktable.append($(markup));
      $table.before($stacktable);
      if (!settings.hideOriginal) $table.show();
    });
  };

  $.fn.stacktable = function(options) {
    var $tables = this,
        defaults = {id:'stacktable small-only',hideOriginal:true,headIndex:0},
        settings = $.extend({}, defaults, options);

    // checking the "headIndex" option presence... or defaults it to 0
    if(options && options.headIndex)
      headIndex = options.headIndex;
    else
      headIndex = 0;

    return $tables.each(function() {
      var table_css = $(this).prop('class');
      var $stacktable = $('<table class="'+ table_css +' '+settings.id+'"><tbody></tbody></table>');
      if (typeof settings.myClass !== 'undefined') $stacktable.addClass(settings.myClass);
      var markup = '';

      $table = $(this);
      $table.addClass('stacktable large-only');
      $caption = $table.find("caption").clone();
      $topRow = $table.find('tr').eq(0);

      // using rowIndex and cellIndex in order to reduce ambiguity
      $table.find('tr').each(function(rowIndex,value) {

        // declaring headMarkup and bodyMarkup, to be used for separately head and body of single records
        headMarkup = '';
        bodyMarkup = '';
        tr_class = $(this).prop('class');
        // for the first row, "headIndex" cell is the head of the table
        if (rowIndex === 0) {
          // the main heading goes into the markup variable
          markup += '<tr class=" '+tr_class +' "><th class="st-head-row st-head-row-main" colspan="2">'+$(this).find('th,td').eq(headIndex).html()+'</th></tr>';
        }
        else {
          // for the other rows, put the "headIndex" cell as the head for that row
          // then iterate through the key/values
          $(this).find('td,th').each(function(cellIndex,value) {
            if (cellIndex === headIndex) {
              headMarkup = '<tr class="'+ tr_class+'"><th class="st-head-row" colspan="2">'+$(this).html()+'</th></tr>';
            } else {
              if ($(this).html() !== ''){
                bodyMarkup += '<tr class="' + tr_class +'">';
                if ($topRow.find('td,th').eq(cellIndex).html()){
                  bodyMarkup += '<td class="st-key">'+$topRow.find('td,th').eq(cellIndex).html()+'</td>';
                } else {
                  bodyMarkup += '<td class="st-key"></td>';
                }
                bodyMarkup += '<td class="st-val '+$(this).prop('class')  +'">'+$(this).html()+'</td>';
                bodyMarkup += '</tr>';
              }
            }
          });

          markup += headMarkup + bodyMarkup;
        }
      });

      $stacktable.prepend($caption);
      $stacktable.append($(markup));
      $table.before($stacktable);
      if (!settings.hideOriginal) $table.show();
    });
  };

 $.fn.stackcolumns = function(options) {
    var $tables = this,
        defaults = {id:'stacktable small-only',hideOriginal:true},
        settings = $.extend({}, defaults, options);

    return $tables.each(function() {
      $table = $(this);
      var num_cols = $table.find('tr').eq(0).find('td,th').length; //first table <tr> must not contain colspans, or add sum(colspan-1) here.
      if(num_cols<3) //stackcolumns has no effect on tables with less than 3 columns
        return;

      var $stackcolumns = $('<table class="'+settings.id+'"></table>');
      if (typeof settings.myClass !== 'undefined') $stackcolumns.addClass(settings.myClass);
      $table.addClass('stacktable large-only');
      var tb = $('<tbody></tbody>');
      var col_i = 1; //col index starts at 0 -> start copy at second column.
      
      while (col_i < num_cols) {
        $table.find('tr').each(function(index,value) {
          var tem = $('<tr></tr>'); // todo opt. copy styles of $this; todo check if parent is thead or tfoot to handle accordingly
          if(index === 0) tem.addClass("st-head-row st-head-row-main");
          first = $(this).find('td,th').eq(0).clone().addClass("st-key");
          var target = col_i;
          // if colspan apply, recompute target for second cell.
          if ($(this).find("*[colspan]").length) {
            var i =0;
            $(this).find('td,th').each(function(index,value) {
                var cs = $(this).attr("colspan");
                if (cs) {
                  cs = parseInt(cs, 10);
                  target -= cs-1;
                  if ((i+cs) > (col_i)) //out of current bounds
                    target += i + cs - col_i -1;
                  i += cs;
                }
                else
                  i++;

                if (i > col_i)
                  return false; //target is set; break.
            });
          }
          second = $(this).find('td,th').eq(target).clone().addClass("st-val").removeAttr("colspan");
          tem.append(first, second);
          tb.append(tem);
        });
        ++col_i;
      }

      $stackcolumns.append($(tb));
      $table.before($stackcolumns);
      if (!(settings.hideOriginal)) {
        $table.show();
      }
    });
  };

}(jQuery));
// this file requires the jquery library (written against 1.2.1)

// anything that needs to be fired off on dom load goes in here
jQuery(document).ready(function() {
	t2eb.init();
 });

// ############################################################
// ## main JS object for t2ebusiness
// ############################################################

 var t2eb =
{

    //log to browser console
    log: function (data)
    {
        if (window.console && window.console.log)
        {
            window.console.log(data);
        }
    },

    //everything that gets kicked off when the dom is loaded goes in init()
    init: function ()
    {

        // #####################################################################
        // prototype us a format() method
        // #ORG move to t2eb.Proto
        String.prototype.format = function ()
        {
            var args = arguments;
            return this.replace(/{(\d+)}/g, function (match, number)
            {
                return typeof args[number] != 'undefined' ? args[number] : match;
            });
        };

        // #####################################################################
        // set up some error handling and defaults for the API calls.
        API.init();

        // #####################################################################
        // attempt to figure out the base URL (it might already be set)
        t2eb.setBaseURL();

        // #####################################################################
        // some marginally useful info
        t2eb.log("Document loaded at " + (new Date).toTimeString());
        t2eb.log("Application BaseURL: " + t2eb.applicationBaseURL);

        // ####################################################################
        // bail out if this is a non-standard page.
        if (jQuery("#t2-container").length === 0) return;

        // ####################################################################
        // draw back button if the wrapper exists
        // #ORG move to t2eb.Form
        //jQuery(".backbuttonWrapper").html("<input type=\"button\" id=\"backbutton\" class=\"btn-back\" value=\"<< Back\" onclick=\"try { workflow(this); } catch (ex) { }; javascript:window.history.back();\" />&nbsp;");

        // ####################################################################
        // collapse the cart
        // #ORG move to t2UI.Cart
        jQuery("span.cart span.cartMinimal").append("<a href=\"#\" id=\"frobCartDetails\">show details</a>");
        jQuery("span.cart a#frobCartDetails").click(function ()
        {
            t2eb.frobCartVisibility();
            return false;
        });
        jQuery("span.cart span.cartDetails").addClass("hidden");

        // ####################################################################
        // if an object with a class of "selectAllWrapper" exists, create a "select all" link.
        // #ORG move to t2eb.Form
        jQuery(".selectAllWrapper").html("<a href=\"javascript:selectAllToggle('true');\">Select All</a>&nbsp;");

        // ####################################################################
        // #ORG move to t2eb.Form
        // if table is a 'select permit' table, select radio by default if there is only one
        if (jQuery("table.selectPermit").length > 0)
        {
            var radioCollection = document.getElementsByName("radChoosePermit");
            if (radioCollection.length == 1)
            {
                radioCollection[0].setAttribute("checked", true);
            }
        }

        // ####################################################################
        // #ORG move to t2UI.Expandable
        // handle show/hide of cart view/receipt detail frobbing
        jQuery("div.moreDetails").hide().each(function (i)
        {
            var toggle = jQuery('<span class="moreDetailsToggle">view details</span>');
            jQuery(this).before(toggle);
            jQuery(toggle).click(function ()
            {
                var details = jQuery(this).next('div.moreDetails');
                jQuery(details).toggle();
                if (jQuery(details).is(':hidden'))
                {
                    jQuery(this).text('view details');
                }
                else
                {
                    jQuery(this).text('hide details');
                }
            });
        });

        // ####################################################################
        // #ORG move to t2UI.Expandable
        // handle show/hide of expansion control
        jQuery('div[class^="expansion-"]').each(function (i)
        {
            $(this).addClass('expansion-closed').removeClass('expansion-open');

            if ($(this).hasClass('eventgroup')) {
                $(this).removeClass('eventgroup');
                $(this).addClass('eventgroup');
            }
            if ($(this).hasClass('event-details')) {
                $(this).removeClass('event-details');
                $(this).addClass('event-details');
            }

            // append the glyph and add the click event handler
            jQuery(this).children('.expansion-title')
				.prepend('<i class="ebicon-plus"></i>')
				.click(function ()
				{
				    jQuery(this).children('i').toggleClass('ebicon-plus ebicon-minus');
				    var $target = jQuery(this).parent('div[class^="expansion-"]');
				    $target.toggleClass('expansion-closed expansion-open');

				    if ($target.hasClass('eventgroup'))
				    {
				        $target.removeClass('eventgroup');
				        $target.addClass('eventgroup');
				    }

				    if ($target.has('event-details'))
				    {
				        $target.removeClass('event-details');
				        $target.addClass('event-details');
				    }
				});
        });

        // ####################################################################
        // #ORG move to t2UI.Expandable
        // For each instructional bit we find...
        jQuery(".hiddenInstructional").each(function (i)
        {
            // Capture its ID
        	var instID = jQuery(this).attr("id");
        	if (typeof(instID) == 'undefined' || instID.length == 0) {
        		// no id on element. give it one.
        		instID = "hiddenInstructional_" + i;
        		jQuery(this).attr("id", instID);
        	}

            // Wrap the content in its own span and hide it
            jQuery(this).wrapInner('<span id="' + instID + '_contents" class="instructionalContents"></span>');
            jQuery('#' + instID + '_contents').hide();

            // Add an image to the hiddenInstructional
            jQuery(this).prepend('<i id="' + instID + '_toggle" class="instructionalToggleControl ebicon-help-circled" style="font-size:1.25em;cursor:help;color:#5589B3;" title="View more details" />');

            // Set the click event for the toggle control...
            jQuery('#' + instID + '_toggle').click(function ()
            {
                var targetId = jQuery(this).attr("id").replace('_toggle', '_contents');
                jQuery("#" + targetId).toggle();
            });
        });

        // #####################################################################
        // mimicing the use of labels on the permit selection table form
        t2eb.tableSelectionHandling();

        // #####################################################################
        // mimicing the use of labels on the permit selection table form
        t2eb.calendarDayClickHandling();

        // #####################################################################
        // Handling clicking of submit button on crt/collect
        t2eb.cartCollectSubmitHandling();

        // #####################################################################
        // handles messages
        t2eb.messageHandler();

        // #####################################################################
        // provides warning for user when session about to time out
        // sessionExtension.init();

        // #####################################################################
        // puts focus on field if field exists
        t2eb.focusAttempt();

        // #####################################################################
        // allows for sorting of tables (by clicking column header) on tables with class of "sortable"
        t2tablesort.init();

        // #####################################################################
        // permit queue init
        t2PermitQueue.init();

        // #####################################################################
        // address control init
        t2AddressControl.init();

        // #####################################################################
        // validate chkAgreement
        $("#cmdNext").click(function () {
            if ($('#ctl00_ctl01_MainContentPlaceHolder_T2Main_chkAgreement').length > 0) {
                return Page_ClientValidate();
            }

            return true;
        });


    	// #####################################################################
    	// event search init
        t2EventSearch.init();

    	// #####################################################################
    	// permitnow init
        PermitNow.init();
    },

    // #ORG move to t2eb.URL
    // get the local application root
    getLocalURL: function (urlTrailing)
    {
        urlTrailing = (!urlTrailing ? "" : urlTrailing);
        if (!urlTrailing.indexOf("/") === 0) urlTrailing = "/" + urlTrailing;

        if (!t2eb.applicationBaseURL)
        {
            t2eb.setBaseURL();
        }
        // applicationBaseURL ends with a "/", so strip the beginning "/" off if we're using that.
        return t2eb.applicationBaseURL + urlTrailing.substring(1, urlTrailing.length);
    },

    // #ORG move to t2eb.URL
    setBaseURL: function ()
    {
        if (t2eb.applicationBaseURL) return;

        var path = location.pathname.split('/');

    	// if the path contains "adm", we're dealing with a tricky directory structure
        var isAdmin = false;
        for (var i = 0, l = path.length; i < l; i++)
        {
            if (path[i] == "adm")
            {
            	path.length = i;
            	isAdmin = true;
                continue;
            }
        }
        // otherwise, it's one less than the .aspx file (hopefully)
        if (!isAdmin && path.length >= 1 && (path[path.length - 1].indexOf('.aspx') > -1 || path[path.length - 1] == ""))
        {
			if (path.length > 1) path.length = path.length - 2;
        }

        var app = path.join('/');
        t2eb.applicationBaseURL = location.protocol + '//' + location.host + app + '/';
        t2eb.log("Application BaseURL (set): " + t2eb.applicationBaseURL);
        return app;
    },

    // #ORG move to t2API
    // construct the API URL
    getAPIURL: function (apiMethod)
    {
        return t2eb.getLocalURL('/adm/API.aspx') + '?m=' + apiMethod;
    },

    // #ORG move to t2eb.Form
    focusAttempt: function ()
    {
        jQuery('#ctl00_ctl01_MainContentPlaceHolder_T2Main_txtLogin1').focus();
        jQuery('#ctl00_T2Main_txtLogin').focus();
        jQuery('#ctl00_T2Main_txtEntityUid').focus();
        jQuery('#ctl00_T2Main_txtSearchText').focus();
        jQuery('#ctl00_T2Main_txtPermitNumber').focus();
        // put other fields you want to receive the focus here.
        // if more than one exists on a page, last one specified will win.
        // if you get more than, say, 10 in here, let me know and we'll refactor this.
    },

    // #ORG move to t2eb.Form
    validateAndShowDialog: function (text, validationGroup)
    {
        if (Page_ClientValidate(validationGroup)) { t2eb.showLoadingDialog(text); }
    },

    // #ORG move to t2UI.Dialog
    showLoadingDialog: function (text)
    {
        var text = (!text ? 'Please Wait...' : text);
        var container = '.t2MessageContainer';
        var dialog =
			"<div class=\"overlay\" id=\"ajaxLoader\">\
                <div class=\"loader\">" + text + "</div>\
            </div>";
        jQuery(container).append(dialog);
    },

    // #ORG move to t2UI.Dialog
    hideLoadingDialog: function ()
    {
        jQuery('.overlay').remove();
    },

    // #ORG move to t2UI.Dialog
    showDeleteConfirmation: function (overrideMessage)
    {
        var message = (!overrideMessage ? "Are you sure you wish to delete this item? This action cannot be undone!" : overrideMessage);
        return confirm(message);
    },

    // #ORG move to t2UI.Modal
    showConfirmationMessage: function (title, text, button1Text, callBack1, button2Text, callBack2)
    {
        // find out how many confirmation dialogs exist so we can give this one a unique id
        var numConfirms = jQuery('.t2MessageContainer div.popConfirmationButtonWrapper').length;
        var unique_id = "t2_pop_confirmation_" + (numConfirms + 1);

        // construct the 'buttons'
        var button1 = (!button1Text ? "" : "<a class='pseudo-button button1' href='#'>" + button1Text + "</a>");
        var button2 = (!button2Text ? "" : "&nbsp;&nbsp;<a class='pseudo-button button2' href='#'>" + button2Text + "</a>");

        // construct the markup
        var confirmationMessage =
			"<div class='popConfirmationWrapper' id='" + unique_id + "'>" +
                "<div class='popConfirmationDialogWrapper'>" + 
    				"<div class='popConfirmationDialog'>" +
	    				"<div class='popConfirmationTitle'>" + title + "</div>" +
		    			"<div class='popConfirmationText'>" + text + "</div>" +
			    		"<div class='popConfirmationButtonWrapper'>" + button1 + button2 + "</div>" +
				    "</div>" +
                "</div>" +
			"</div>";
        // find the message container by class name (since .NET likes to change our IDs)
        jQuery('.t2MessageContainer').append(confirmationMessage);
        // if callBack is set, assign it to the click event
        if (callBack1)
        {
            jQuery('#' + unique_id + ' div.popConfirmationButtonWrapper a.pseudo-button.button1').click(callBack1);
        }
        if (callBack2)
        {
            jQuery('#' + unique_id + ' div.popConfirmationButtonWrapper a.pseudo-button.button2').click(callBack2);
        }
        t2eb.log("showConfirmationMessage created message with id of " + unique_id);

        // run the messageHandler() function again to catch this one.
        t2eb.popConfirmationHandler(unique_id);
        return unique_id;
    },

    // #ORG move to t2UI.announcement
    announcementFrobbing: function ()
    {
        //jQuery("a.announcement span.announcementTitle").append('<span class="announcementClickForMore">read announcement</span>');

        jQuery("a.announcementLink").click(function (e)
        {
            var $announcementContainer = jQuery(this).parent();
            var $announcementMessage = $announcementContainer.children("span.announcementMessage");
            var $announcementTitle = $announcementContainer.children("span.announcementTitle");
            var $announcementLink = $announcementContainer.children("a.announcementLink");
            if ($announcementMessage.hasClass("hidden"))
            {
                $announcementMessage.removeClass("hidden");
                $announcementLink.text("hide announcement");
            }
            else
            {
                $announcementMessage.removeClass("hidden").addClass("hidden");
                $announcementLink.text("read announcement");
            }

            e.preventDefault();
        });
    },

	calendarDayClickHandling: function ()
	{
		// TODO: add click event on ".calendarDay" to run .showLoadingDialog("...");
		jQuery(".calendarContainer .calendarDay").click(function(e)
		{
			t2eb.showLoadingDialog("Calculating Capacities");
		});
	},

	cartCollectSubmitHandling: function ()
	{
	    jQuery(".ipgSubmit").click(function (e)
	    {
	        var url = t2eb.getLocalURL("/crt/collect.aspx/UpdateBasketStatus");


	        jQuery.ajax({
	            url: url,
	            type: 'POST',
	            // running this asynchronously means that it may not get run
                // if the user is redirected before it can run.
                async: false,
	            contentType: "application/json; charset=utf-8",
	            dataType: "json",
	            success: function (data)
	            {
	                if (data.d.status == "1")
	                {
	                    return true;
	                }
	                else
	                {
	                    e.preventDefault();
	                    window.location = t2eb.getLocalURL(data.d.status);
	                    return false;
	                }
	            }
	        });
	    });
        
	},

    // #ORG move to t2eb.Form
    tableSelectionHandling: function ()
    {
        // when a row in a tableSelection table is clicked, look for an input to check/uncheck
        jQuery("table.tableSelection tr td").click(function ()
        {
            // if the td clicked contains a radio/checkbox/select, stop here
            if (jQuery(this).children("input").length > 0 || jQuery(this).children("select").length > 0)
            {
                return;
            }

            // get the parent (tr)
            tr = jQuery(this).parent("tr");

            //look for a radio input
            jQuery(tr).children("td").each(function ()
            {
                var input = jQuery(this).children("input");
                if (input.length == 0)
                {
                    // no input in this td, keep looking
                    return true; // this acts like a continue statement
                }
                else
                {
                    input = input[0];
                    if (jQuery(input).is(":checked"))
                    {
                        jQuery(input).attr('checked', false);
                    }
                    else
                    {
                        jQuery(input).attr("checked", "checked");
                    }
                    return false; // this acts like a break statement
                }
            });
        });
    },

    // #ORG move to t2UI.Cart
    frobCartVisibility: function ()
    {
        if (jQuery("span.cart span.cartDetails").hasClass("hidden"))
        {
            // cart details are hidden, show them.
            jQuery("span.cart span.cartDetails").removeClass("hidden");
            jQuery("span.cart a#frobCartDetails").text("hide details");

        }
        else
        {
            jQuery("span.cart span.cartDetails").addClass("hidden");
            jQuery("span.cart a#frobCartDetails").text("show details");
        }
    },

    // #ORG move to t2eb.Form
    selectAllToggle: function (toggle)
    {
        var myForm = document.forms[0];
        for (var i = 0; i < myForm.length; i++)
        {
            if (toggle)
            {
                myForm.elements[i].checked = "checked";
            }
            else
            {
                myForm.elements[i].checked = "";
            }
        }
    },

    // #ORG move to t2UI.Message
    messageHandler: function ()
    {

        // #####################################################################
        // handles dismissal of the popConfirmationDialog (Message.MessageType.Confirm)
        t2eb.popConfirmationHandler();

        // #####################################################################
        // handles timed fadeout of Message.MessageType.Temporary
        t2eb.temporaryMessageHandler();

        // #####################################################################
        // handles expanding/collapsing of announcement text
        t2eb.announcementFrobbing();

        // expand/collapse details area
        jQuery('<a href="#" class="detailFrobber">view details</a>').insertBefore('div.message div.details');
        if (jQuery('div.messageText').children('div.details').length > 0)
        {
            jQuery('div.messageText').addClass("hiddenDetails");
            jQuery('div.message a.detailFrobber').click(function ()
            {
                d = jQuery(this).parent('div.messageText');

                if (jQuery(d).hasClass("hiddenDetails"))
                {
                    jQuery(d).removeClass("hiddenDetails").removeClass('visibleDetails').addClass('visibleDetails');
                    jQuery(this).text("hide details");
                }
                else
                {
                    jQuery(d).removeClass("hiddenDetails").removeClass('visibleDetails').addClass("hiddenDetails");
                    jQuery(this).text("view details");
                }
                return false;
            });
        }

        // #####################################################################
        // checks for certain messages that should be removed from the DOM under
        // certain circumstances

        if (jQuery('div#message_EED5DF22280D80CC404D9D2626258E15C09235C3').length > 0)
        {
            // if the error message "You must enter at least one phone number!" exists, remove the 
            // message the information message "At least one phone number must be provided"
            jQuery('div#message_C2CDC342517F47BB35F3E96811DA4C93A6EA0399').remove();
        }

    },

    // #ORG move to t2UI.Modal
    popConfirmationHandler: function (id)
    {
        // attach hiding of confirmation to button
        if (id)
        {
            t2eb.log("popConfirmationHandler prepping handling for id of " + id);
            jQuery('#' + id + ' div.popConfirmationButtonWrapper a').click(function ()
            {
                t2eb.log("popConfirmationHandler hiding id of " + id);
                jQuery('#' + id).hide();
                return false;
            });
        }
        else
        {
            t2eb.log("popConfirmationHandler prepping handling for ALL popConfirmationWrappers");
            jQuery('div.popConfirmationButtonWrapper a').click(function ()
            {
                t2eb.log("popConfirmationHandler hiding all popConfirmationWrappers");
                jQuery(this).parents('div.popConfirmationWrapper').hide();
                return false;
            });
        }

        // put focus on confirmation button
        jQuery('div.popConfirmationButtonWrapper a').focus();
    },

	// #ORG move to t2UI.Message
    generateToast: function(message, ttl)
    {
    	var toastId = "toast" + new Date().valueOf();

    	jQuery('#t2-container')
			.append('<div class="toast" id="' + toastId + '"><span>' + message + '</span></div>');

    	if (!ttl) ttl = 0;
    	if (ttl > 0)
    	{
    		jQuery('#' + toastId).animate({ nothing: 1 }, ttl).fadeOut(1000);
    	}

    	return toastId;
    },

	// #ORG move to t2UI.Message
    destroyToast: function(toastId)
    {
    	jQuery('#'+toastId).remove();
    },

    // #ORG move to t2UI.Message
    temporaryMessageHandler: function ()
    {
        // if there are multiple temporary messages, bump consecutive ones down in the UI
        var tempCount = jQuery('div.message.temporary').length;
        if (tempCount > 1)
        {
            jQuery('div.message.temporary').each(function (i)
            {
                jQuery(this).css({ marginTop: (2.5 * i) + 'em' });
            });
        }
        // add the delay and the animation
        jQuery('div.message.temporary').animate({ nothing: 1 }, 5000).fadeOut(1000);
        return false;
    },

    // #ORG move to t2UI.Form (is this even used anymore? why are there other elements mentioned in here?)
    // simple validate form for email
    //when adding "validateForm()" you must remove the onClick event from the submit button
    validateEmail: function (f)
    {
        var strEmail = f.EMail.value

        if (strEmail.length < 3 || strEmail.indexOf("@") == -1 || strEmail.indexOf(".") == -1)
        {
            alert("You must enter a valid email address!");
            f.EMail.focus();
            return false;
        }
        document.getElementById('ccExternalButtons').style.display = 'none';
        document.getElementById('waitDiv').style.display = 'block';
        return true;
    },
	
    // ####################################################################
    // #ORG move to t2UI.Expandable
    // handle show/hide of all expansion controls
    expandAll: function () {
        $('div[class^="expansion-"]').each(function (i) {
            t2eb.expandElement(this);
        });
    },

    expandElement: function (obj) {
        $(obj).children('.expansion-title').each(function () {
            $(this).addClass('expansion-open').removeClass('expansion-closed');
            $(this).children('i').addClass('ebicon-minus').removeClass('ebicon-plus');
            var $target = $(this).parent('div[class^="expansion-"]');
            $target.addClass('expansion-open').removeClass('expansion-closed');

            if ($target.hasClass('eventgroup')) {
                $target.removeClass('eventgroup');
                $target.addClass('eventgroup');
            }

            if ($target.has('event-details')) {
                $target.removeClass('event-details');
                $target.addClass('event-details');
            }
        });
    },

    collapseAll: function () {
        $('div[class^="expansion-"]').each(function (i) {
            t2eb.collapseElement(this);
        });
    },

    collapseElement: function (obj) {
        $(obj).children('.expansion-title').each(function () {
            $(this).addClass('expansion-closed').removeClass('expansion-open');
            $(this).children('i').addClass('ebicon-plus').removeClass('ebicon-minus');
            var $target = $(this).parent('div[class^="expansion-"]');
            $target.addClass('expansion-closed').removeClass('expansion-open');

            if ($target.hasClass('eventgroup')) {
                $target.removeClass('eventgroup');
                $target.addClass('eventgroup');
            }

            if ($target.has('event-details')) {
                $target.removeClass('event-details');
                $target.addClass('event-details');
            }
        });
    },
};

var PermitNow =
{

	// create an object to store data about the selected event values
	EventValueData: function ()
	{
		Title = "";
		Price = 0;
		Quantity = 1;
	},

	init: function () {

		PermitNow.EventValueData.prototype.getTotal = function () {
			return this.Price * this.Quantity;
		}

		PermitNow.toggleEventValueQuantity();

		$(".event-value-quantity").css({ "display": "none" });

		if (
			$('#pnlEventSelection').find('.eventgroup').length == 1
			|| $('#pnlEventDates').find('.event-details').length == 1
			) {
			t2eb.expandAll();
		}

		$('.event-value-checkbox input:checked').each(function (i) {
		    PermitNow.selectEventValue(this);
		    var eventDetailsElement = $(this).closest('.event-details');
		    t2eb.expandElement(eventDetailsElement);
		});
	},

	selectEventValue: function (obj) {
		PermitNow.toggleEventValueQuantity(obj);
		PermitNow.updateSelectedEventSummary(obj);
	},

	selectEventValueQuantity: function (obj) {
		PermitNow.updateSelectedEventSummary(obj);
	},

	updateSelectedEventSummary: function (obj) {

		var $summaryContainer = $('#event-value-selection-summary-details');
		var grand_total = 0;
		var eventValueDataCollection = [];

		// loop over a collection of all the checked event value checkboxes:
		$('.event-value-checkbox input:checked').each(function (i) {
			$event_container = $(this).parent(); // get parent container for the event selections
			ev = new PermitNow.EventValueData(); // initialize a new EventValueData object

			// get the title out of the DOM
			var title = $event_container.nextAll('.event-value-title').text();
			ev.Title = title;

			// get the price out of the DOM
			var price = $event_container.nextAll('.event-value-price').text().replace('$', '');
			ev.Price = price;

			// get the selected quantity out of the DOM
			var quantity = $event_container.nextAll('.event-value-quantity').children('.event-value-selectedquantity').val();
			ev.Quantity = quantity;

			t2eb.log(ev); // DEBUG!
			eventValueDataCollection.push(ev); // add the EventValueData to the collection

			grand_total += ev.getTotal(); // increment our grand total

			t2eb.log(grand_total); // DEBUG!
		});

		// DEBUG!
		t2eb.log($('.event-value-checkbox input:checked').length + ' checkboxes are checked');

		// now put it all together to generate a summary.
		var summary_html = "Total Selected: $" + grand_total + ".";

		// we want to inject our summary into two DOM elements. One ($summaryContainer), which 
		// wraps the "Add to Basket" button, and another that will float by the "just selected" 
		// event value (by float I mean z-indexed above and abolutely positioned in a relatively
		// positioned .event-value-details element). The former will also inform the user that they 
		// should click the button when they're ready to proceed and the latter will provide a link to 
		// #event-value-selection-summary when they're ready to proceed.
		$summaryContainer.html(PermitNow.generateSummaryView(eventValueDataCollection));

		if (typeof (obj) != 'undefined')
		{
			$(".event-value-summary-dialog").remove(); // remove any existing ones
			$summaryDialog = $('<div class="event-value-summary-dialog"></div>'); // create a container
			var summaryDialogContents = PermitNow.generateSummaryDialog(eventValueDataCollection, obj); // generate contents
			if (summaryDialogContents.length > 0) {
				$summaryDialog.html(summaryDialogContents); // place contents into container
				$(obj).parent().parent().append($summaryDialog); // append container to dom
			}
		}
	},

	toggleEventValueQuantity: function (obj) {
		var checkbox = $(obj);
		var parent = checkbox.closest('.event-value-details');
		var quantity = parent.find('.event-value-quantity');

		if (checkbox.is(":checked")) {
			quantity.fadeIn();
		}
		else {
			quantity.fadeOut();
			quantity.css({ "display": "none" })
		}
	},

	generateSummaryView: function(eventValueDataCollection)
	{
		var html = "You've selected the following: <br />";
		var grand_total = 0;
		for (var i = 0, l = eventValueDataCollection.length; i < l; i++)
		{
			var currentVal = eventValueDataCollection[i];
			var eventValueDataDescription = "";
			eventValueDataDescription += currentVal.Title
                + ": " + currentVal.Quantity + " @ $" + this.roundCurrency(currentVal.Price).toFixed(2) + "/each: "
				+ " $" + this.roundCurrency(currentVal.getTotal()).toFixed(2) + "<br />";
			grand_total += currentVal.getTotal();
			html += eventValueDataDescription;
		}
		if (html.length == 0) {
			html = "<p>You haven't selected anything</p>";
		} else {
			html += "<strong>Total: $" + this.roundCurrency(grand_total).toFixed(2) + "</strong><br />";
		}
		html += "<p>All finished? Click the button below to add your selections to your basket.</p>";
		return html;
	},

	generateSummaryDialog: function(eventValueDataCollection, obj)
	{
	    var html = "";
	    if (eventValueDataCollection.length > 0) {
	        // get the just-selected event.
	        var event_parent = $(obj).parents('.event-value-details').first();
	        var current_price = $(event_parent).children('.event-value-price').first().text().replace('$', '');
	        var current_quantity = $(event_parent).children('.event-value-quantity').first().children('.event-value-selectedquantity').val();
	        var current_title = $(event_parent).children('.event-value-title').first().text();
	        var current_total = this.roundCurrency(current_price * current_quantity);

            // now get the total for all selected events.
	        var total = 0.0;
	        for (var i = 0, l = eventValueDataCollection.length; i < l; i++) {
	            total += eventValueDataCollection[i].getTotal();
	        }

	        // generate html.
	        html += "<div class=\"event-value-summary-dialog-close-button\"><a href=\"#\" onclick=\"$('.event-value-summary-dialog').hide();return false;\">Close</a></div>";
	        html += "<div>";
	        html += "This item: <br /><strong>" + current_title + "</strong>: " + current_quantity + " @ $" + this.roundCurrency(current_price).toFixed(2) + "/each: $" + current_total.toFixed(2);
	        html += "<br /><br />Total Items Selected: $" + this.roundCurrency(total).toFixed(2) + "<br /><br />";
	        html += "Continue selecting or <a href='#event-value-selection-summary'>Review your selections and Continue</a>";
	        html += "</div>";
	    }
		return html;
	},

	roundCurrency: function (num) {
        // multiply by 100, round, then divide by 100.
	    return Math.round(num * Math.pow(10, 2)) / Math.pow(10, 2);
	},

	closeEventDialog: function ()
	{

	}
};

// #ORG rename to t2API
var API =
{
	status: 'OK',
	last_error_response: '',

	init: function ()
	{
		API.setup();
	},

	setup: function ()
	{
		var status = '';

		jQuery(document).ajaxError(function (event, request, settings)
		{

			// offline
			if (request.status == 0) status = 'DISABLE';
			// unauthorized
			else if (request.status == 403) status = 'DISABLE';
			// 404, not found
			else if (request.status == 404) status = 'DISABLE';
			// internal server error. we can try again.
			else if (request.status == 500) status = 'ERROR';
			// couldn't parse response. we can try again.
			else if (event != undefined && event.type == 'parsererror') status = 'ERROR';
			// request timed out, we can try again.
			else if (event != undefined && event.type == 'timeout') status = 'ERROR';
			// some other error, we can try again.
			else status = 'ERROR';

			API.status = status;
			API.last_error_response = event.responseText;
			t2eb.log("API status = " + API.status + " (" + API.last_error_response + ")");
		});
	},

	post: function (url, method, parameters, callback)
	{
		if (!url) url = '/adm/API.aspx';
		url = t2eb.getLocalURL(url) + '/' + method;
		jQuery.ajax({
			type: "POST",
			url: url,
			contentType: "application/json; charset=utf-8",
			data: parameters,
			dataType: "json",
			success: callback
		});
	},

	get: function (url, method, parameters, callback, errorCallback)
	{
		if (!url) url = '/adm/API.aspx';
		url = t2eb.getLocalURL(url) + '?m=' + method;

		//jQuery.getJSON(url, parameters, callback);

		jQuery.ajax({
			url: url,
			data: parameters,
			dataType: 'json',
			success: callback,
            cache: false,
			timeout: 60000,	// 60000 = 60 seconds
			error: function (jqXHR, status, errorThrown)
			{
				if (errorCallback) errorCallback(jqXHR, status, errorThrown);
			}
		});
	}
}

// #ORG move to t2eb.Form
var t2tablesort =
{

	init: function ()
	{
		// find tables that meet our criteria
		// must use "tsuperheader" for header..
		jQuery("table.sortable").each(function (i)
		{
			var $table = jQuery(this);

			// get the table header (and hold onto it, we'll need it later
			var $tableHeader = $table.find('tr.tsuperheader,tr.tableHeader');

			// loop over each column in the tableHeader (looks like we'll need to support th AND td; great)
			$tableHeader.children('th,td').each(function (i)
			{
				if (jQuery(this).children("span.sort").length < 1)
				{
					jQuery(this).append('<span class="sort" />');
				}
				jQuery(this).click(function ()
				{
					var dir = "sortAsc";

					// determine sort to use
					if (jQuery(this).hasClass("sortAsc"))
					{
						dir = "sortDesc";
					}

					// remove all current sorts
					$tableHeader.children('th,td').removeClass("sortAsc").removeClass("sortDesc");

					// apply the new sort class
					jQuery(this).addClass(dir);

					// identify the column we're sorting on
					var col_index = $tableHeader.children('th,td').index(jQuery(this));

					// actually DO THE SORT
					t2tablesort.sort($table, col_index, dir);
				});
			});
		});
	},

	sort: function ($table, col_index, dir)
	{
		// get all the rows from the table (except the header)
		$rows = $table.find('tr').not('tr.tsuperheader,tr.tableHeader,tr.theader,tr.tsubheader');

		t2eb.log("t2tablesort: sorting column " + col_index + " by " + dir);

		// sort the values in the proper table cell
		$rows.sort(function (a, b)
		{
			var a_val = jQuery(a).children('td').eq(col_index).text();
			var b_val = jQuery(b).children('td').eq(col_index).text();
			if (dir == 'sortAsc')
			{
				return (a_val > b_val ? 1 : -1);
			}
			else
			{
				return (a_val < b_val ? 1 : -1);
			}
		}).each(function (i)
		{
			// move the rows (in order) onto the bottom of the table
			$table.append(jQuery(this));
		});
		return false;
	}

}

// #ORG move to t2eb.PermitQueue
var t2PermitQueue =
{
	targetId: '#txtPermitQueuePosition',
	timer: null,
	delay: 15000, 			//in milliseconds, default 15s

	init: function ()
	{

		if (jQuery(t2PermitQueue.targetId).length > 0)
		{
			t2PermitQueue.timer = setInterval(t2PermitQueue.getPosition, t2PermitQueue.delay);
		}
	},
	getPosition: function ()
	{
		API.get('/adm/API.aspx', 'get_queue_position', '', function (response)
		{
			if (!response || response.status != 0)
			{
				t2eb.log("Error from API, No queue position available");
				return;
			}

			var queueStatus = response.data;
			t2eb.log(queueStatus);
			t2eb.log("API says position in the queue is " + queueStatus.MyPosition + ", depth of " + queueStatus.Depth);

			if (queueStatus.MyPosition == "0") window.location.reload(true);	

			jQuery(t2PermitQueue.targetId).text(queueStatus.MyPosition + 1);
		});
	}
}

var t2AddressControl = t2AddressControl || {};
t2AddressControl.init = function ()
{
	t2AddressControl.Bind();
}
t2AddressControl.ContainsData = function ()
{
	var totalLength =
			$("#txtAddress1").val().length
			+ $("#txtCity").val().length
			+ $("#txtPostalCode").val().length;

	if (totalLength > 0)
	{
		return true;
	}
	return false;
};
t2AddressControl.SetRequired = function (req)
{
	t2eb.log("set required: " + req);
	if (req && $("span.dynamic").length == 0)
	{
		$("#txtAddress1").after("<span class='requiredmark dynamic'>*</span>");
		$("#txtCity").after("<span class='requiredmark dynamic'>*</span>");
		$("#txtPostalCode").after("<span class='requiredmark dynamic'>*</span>");
	}
	else
	{
		$("span.dynamic").remove();
	}
};
t2AddressControl.CheckRequired = function ()
{
	if (t2AddressControl.ContainsData())
	{
		t2AddressControl.SetRequired(true);
	}
	else
	{
		t2AddressControl.SetRequired(false);
	}
};
t2AddressControl.Bind = function ()
{
	// if not already required, check to see if it becomes 
	// required... if txtAddress1_requiredIndicator exists,
	// it is already required
	var isRequired = ($("#txtAddress1_requiredIndicator").length > 0);
	if (!isRequired)
	{
		// must bind-by-delegate for the agent/managePermit, since #txtAddress 
		// does not exist at the time of binding.
		if ($(".associatedObjects").length > 0)
		{
			$(".associatedObjects").on("change", ".minRequired", function (e)
			{
				t2AddressControl.CheckRequired();
			});
			t2eb.log("Bound CheckRequired to change event (delegate)");
		}
		else
		{
			$(".minRequired").on("change", function (e)
			{
				t2AddressControl.CheckRequired();
			});
			t2eb.log("Bound CheckRequired to change event (direct)");
		}
	}
};

var t2EventSearch = {};
(function () {
    this.init = function () {
        // ### Hide the search and clear buttons
        $("#cmdSearch").hide();
        $("#cmdClearSearch").hide();

        // ### Prevent enter from submitting on the form        
        $('#txtEventSearchText').on('keydown', function (e) {
            var code = e.keyCode || e.which;
            if (code == 13) {
                e.cancelBubble = true;
                e.preventDefault();
                return false;
            }
        });

        this.GetAutoCompleteList();

        // ### Bind all event handlers
        this.BindSearchToInput();
    },

    // ### Sets the text on the feedback element to the right of the search box
    this.SetSearchStatus = function (text) {
        if (!text || text.length == 0) {
            $('#searchStatus i').hide().text('');
            return;
        }

        $('#searchStatus i').show().text(text);
    }

    // ### Binds events related to the searching
    this.BindSearchToInput = function () {
        // ### Bind the keyup event on the search box
        $('#txtEventSearchText').on('keyup', t2EventSearch.SearchEventHandler);
        $('#txtEventSearchText').on('autocompleteclose', t2EventSearch.SearchEventHandler);

        // ### Focus the search box
        $('#txtEventSearchText').focus();
    }

    // ### Handles requests to perform the search based on the contents of the search text box
    this.SearchEventHandler = function (e) {
        if ($('#txtEventSearchText').val().length > 0) {
            t2EventSearch.SetSearchStatus('searching...');
        }
        else {
            t2EventSearch.SetSearchStatus('');
        }

        // ### Clear any current event timeout
        clearTimeout($(e.target).data('timeout'));
        // ### Set a 3 second timeout on the keypress
        var e = $(e.target).data('timeout',
        setTimeout(function () {
            t2EventSearch.SearchGridForText(e.val());
        }, 500));

        // ### Fadeout search status
        setTimeout($('#searchStatus i').delay(5000).fadeOut());
    }

    // ### Performs the requested search
    this.SearchGridForText = function (text) {
        // ### Case insenstive searches
        text = text.toLowerCase();

        var eventGroups = $('#pnlEventSelection').find('.eventgroup');

        // ### Run the search
        // ### Iterate over each event group
        var groupsHidden = 0;
        var groupsCount = eventGroups.children().length;

        $(eventGroups).each(function (i) {
            var events = $(this).find('.expansion-data').children();
            var eventsCount = 0;
            var match = false;
            var matchCount = 0;

            // ### Iterate over each event in event group
            $(events).each(function (j) {
                var eventChildren = $(this).children();
                // ### Iterate over each element in the event and determine if there is a match
                eventChildren.each(function (k) {
                    var cString = $(this).text();

                    if (cString.toLowerCase().indexOf(text) > -1) {
                        match = true;
                        matchCount++;
                        return false;
                    }
                    else {
                        match = false;
                    }
                });

                // ### If there was no match, hide the event
                if (!match) {
                    $(this).addClass('search-hide');
                }
                else {
                    $(this).removeClass('search-hide');
                }
            });

            // ### If no events matched, hide the group
            if(matchCount == 0) {
                $(this).addClass('search-hide');
                groupsHidden++;
            }
            else {
                $(this).removeClass('search-hide');
            }

            matchCount = 0;
        });
        
        if (groupsHidden == eventGroups.length)
        {
            t2EventSearch.SetSearchStatus('No results.');
        }

        if ((eventGroups.length - groupsHidden) == 1)
        {
            t2eb.expandAll();
        }
        else {
            t2eb.collapseAll();
        }
    }

    // ### Retrieves a list of events from hidden field and sets them as the autocomplete menu for the search control.
    this.GetAutoCompleteList = function () {
        if ($('#txtEventSearchText').length < 1) return;

        var list = $('#autocompleteList').val();
        var jsonList = $.parseJSON(list);

        $('#txtEventSearchText').autocomplete({
            source: jsonList,
            response: function (event, ui) {
                if (ui.content.length === 0) {
                    t2EventSearch.SetSearchStatus('');
                }
            }
        });
    }
}).apply(t2EventSearch);
'use strict';

var authTokenHandler = (function ($) {

    function getToken() {
        return $.cookie("apiToken");
    }

    function hasValidCredentials() {
        var token = getToken();
        if (typeof (token) == 'undefined' || token == null) return false;
        return true;
    }

    function clearAuthData() {
        $.removeCookie("apiToken", { path: '/' });
    }

    function addAuthHeader(xhr) {
        
        if (!hasValidCredentials()) {
            return;
        }

        var auth = getToken();
        var header = "Basic " + auth;
        xhr.setRequestHeader('Authorization', header);
    }

    function authErrorHandler(xhr) {
        console.log(xhr);
        if (xhr.status == '401' && hasValidCredentials()) {
            //clearAuthData();
        }
    }

    function init() {
        $.ajaxSetup({
            beforeSend: addAuthHeader,
            error: authErrorHandler
        });
    }

    init();

    return {
        getToken: getToken,
        hasValidCredentials: hasValidCredentials
    }

})(jQuery);
